<div class="list-content">
    <div class="container">
        <div class="row news">
            <div class="col-lg-12 col-md-12 col-sm-12 blog-list">
                <ul>
                    <% page.posts.each(function(post, i){ %>
                        <li class="news-car">
                            <div class="news-content">
                                <h5><a class="title" href="<%- url_for(post.path) %>"><%= post.title %></a></h5>
                                <p>
                                    <%- excerpt(post) %>
                                </p>
                                <span><%= date(post.date) %></span>
                            </div>
                        </li>
                    <%})%>
                </ul>
            </div>
        </div>
    </div>
</div>

<div class="container">
    <%- bootstrap_paginator({
        prev_text: "Previous",
        next_text: "Next"
    }) %>
</div>